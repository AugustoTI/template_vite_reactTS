{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "soucer.fixAll.eslint": "explicit"
  },
  "prettier.configPath": "./.prettierrc.json",
  "editor.formatOnSave": true,
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["cx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "editor.quickSuggestions": {
    "strings": "on"
  }
}
